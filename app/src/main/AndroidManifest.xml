<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tt">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="College Comrade"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PdfViewerActivity"></activity>
        <activity android:name=".PdfReaderActivity" />
        <activity android:name=".eec" />
        <activity android:name=".todo.TaskAddingActivity" />
        <activity android:name=".todo.TodoRV.TodoListActivity" />
        <activity android:name=".oop" />
        <activity android:name=".dsa" />
        <activity android:name=".math" />
        <activity android:name=".Lecturenotes" />
        <activity android:name=".timetable" />
        <activity android:name=".syllabus" />
        <activity android:name=".holiday" />
        <activity android:name=".faculty" />
        <activity android:name=".PROFILE" />
        <activity android:name=".contact" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>